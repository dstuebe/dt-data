{
    "document" : "torque.xml",
    "vars" : {
        "container_specs": {
          "epuworker_container": [
            {
              "name": "epu-worker", 
              "args": { }, 
              "config": { 
                "epu.ionproc.epu_worker":{ "queue_name_work":"${queue_name_work}" }
              },
              "version": "0.1"
            }
          ]
        },
        "local_app_confs": { }
    },
    "sites" : {
        "ec2-west": {
            "work_consumer": {
                "image": "${epuworker_image_id}", 
                "allocation": "m1.large",
                "sshkeyname": "ooi"
            },
            "micro_work_consumer": {
                "image": "${epuworker_image_id}",
                "allocation": "t1.micro",
                "sshkeyname": "ooi"
            }
        },
        "nimbus-ucsd": {
            "work_consumer": {
                "image": "${epuworker_image_id}", 
                "allocation": "m1.large",
                "sshkeyname": "ooi"
            }
        }
    }
}
