{
    "document" : "generic_deployable_type.xml",
    "vars" : { 
        "container_specs": {
          "exchange_mgmt_container": [ 
            {
                "name":"exchange_management",  
                "version":"0.1",
                "processapp":["exchange_management","ion.services.coi.exchange.exchange_management","ExchangeManagementService"]
            }
          ],
          "preservation_container": [ 
            {
                "name":"preservation_manager",  
                "version":"0.1",
                "processapp":["resource_registry","ion.services.dm.preservation.cassandra_manager_agent","CassandraManagerService"],
                "config":{}
            }
          ]
        },
        "local_app_confs": { }
    },
    "sites" : {
        "ec2-west": {
            "work_consumer": {
                "image": "${epuworker_image_id}", 
                "allocation": "m1.large",
                "sshkeyname": "ooi"
            }
        },
        "nimbus-ucsd": {
            "work_consumer": {
                "image": "${epuworker_image_id}", 
                "allocation": "m1.large",
                "sshkeyname": "ooi"
            }
        }
    }
}
